        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Create Excel" Icon="Icons\Employee.ico" Height="215" Width="385" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Enter your Employee ID" Height="28" HorizontalAlignment="Left" Margin="20,57,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="178,59,0,0" Name="txtEmployeeId" VerticalAlignment="Top" Width="120" TextChanged="txtEmployeeId_TextChanged" />
        <Button Content="Continue" Height="23" HorizontalAlignment="Left" Margin="141,116,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
    </Grid>
</Window>